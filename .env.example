# Discord Bot Configuration
# Replace with your Discord bot token
DISCORD_TOKEN=INSERT_YOUR_DISCORD_BOT_TOKEN_HERE

# Roblox Integration Configuration (Optional)
# Replace with your Roblox cookie for group management
ROBLOX_COOKIE=YOUR_ROBLOX_COOKIE_HERE
# Replace with your Roblox group ID for promotions
ROBLOX_GROUP_ID=YOUR_GROUP_ID_HERE

# Optional Configuration
# Set to 'production' for production deployment
NODE_ENV=development
# Log level: debug, info, warn, error
LOG_LEVEL=info